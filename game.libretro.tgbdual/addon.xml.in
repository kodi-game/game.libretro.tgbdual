<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.tgbdual"
		name="Game Boy / Game Boy Color (TGB Dual)"
		version="0.8.3"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>gb|gbc</platforms>
		<extensions>gb|gbc|sgb</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<supports_keyboard>false</supports_keyboard>
		<supports_mouse>false</supports_mouse>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Game Boy / Game Boy Color (TGB Dual) v0.8.3 7e54adc</summary>
		<description>Source: https://github.com/libretro/tgbdual-libretro

Supported files: .gb, .gbc, .sgb</description>
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
	</extension>
</addon>